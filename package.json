{
  "name": "vidom",
  "version": "0.0.36",
  "description": "",
  "keywords": [
    "virtual",
    "dom",
    "component"
  ],
  "main": "lib/vidom.js",
  "author": "Dmitry Filatov <dfilatov@yandex-team.ru>",
  "repository": {
    "type": "git",
    "url": "git://github.com/dfilatov/vidom.git"
  },
  "dependencies": {
    "babelify": "6.1.3",
    "envify": "3.4.0"
  },
  "devDependencies": {
    "browserify": "10.2.1",
    "expect.js": "0.3.1",
    "jscs": "^2.1.0",
    "mocha": "2.2.5",
    "mocha-phantomjs": "3.5.3",
    "phantomjs": "1.9.17",
    "simulate": "git://github.com/dfilatov/simulate.js",
    "sinon": "1.15.4",
    "uglify-js": "2.4.24",
    "zuul": "3.0.0"
  },
  "license": "MIT",
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "scripts": {
    "lint": "jscs lib spec/*/*",
    "test": "browserify spec/boot.js --outfile spec/boot.build.js && mocha-phantomjs spec/runner.html",
    "test-browsers": "NODE_ENV=production zuul -- spec/boot.js",
    "test-server": "zuul --local 3000 -- spec/boot.js",
    "build-dist": "mkdir -p dist && NODE_ENV=production browserify lib/vidom.js -t babelify -t envify --outfile dist/vidom.js --standalone vidom && uglifyjs dist/vidom.js -c -m -o dist/vidom.min.js",
    "build-example": "NODE_ENV=development browserify example/vidom.js --outfile example/vidom.build.js",
    "build-cmp": "browserify example/cmp.js --outfile example/cmp.build.js",
    "build-demo": "browserify demo/demo.js --outfile demo/demo.build.js"
  }
}
